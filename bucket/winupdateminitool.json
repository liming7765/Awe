{
    "version": "1.0.9462",
    "description": "Windows Update Mini Tool is a tool to manage updates of Microsoft products on the Windows operating system",
    "homepage": "https://github.com/sergiye/winUpdateMiniTool",
    "license": {
        "identifier": "GPL-3.0 license",
        "url": "https://github.com/sergiye/winUpdateMiniTool#GPL-3.0-1-ov-file"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/sergiye/winUpdateMiniTool/releases/download/1.0.9462/winUpdateMiniTool.zip",
            "hash": "这里填入你计算出的zip文件的SHA256值"
        }
    },
    "bin": "winUpdateMiniTool.exe",
    "shortcuts": [
        [
            "winUpdateMiniTool.exe",
            "winUpdateMiniTool"
        ]
    ],
    "persist": "winUpdateMiniTool.ini",
    "post_install": [
        "if (!(Test-Path \"$dir\\winUpdateMiniTool.ini\")) { New-Item \"$dir\\winUpdateMiniTool.ini\" -ItemType File | Out-Null }"
    ],
    "checkver": {
        "url": "https://api.github.com/repos/sergiye/winUpdateMiniTool/releases",
        "jp": "$[0].assets[*].browser_download_url",
        "regex": "/releases/download/([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/sergiye/winUpdateMiniTool/releases/download/$version/winUpdateMiniTool.zip"
            }
        }
    }
}
